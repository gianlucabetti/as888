012600050301             chgvar     var(%sst(&G02PAR  1 8))   value(&G02DAT)
012700050301             chgvar     var(%sst(&G02PAR  9 8))   value(&G02INV)
012800050301             chgvar     var(%sst(&G02PAR 17 1))   value(&G02ERR)
