005300050301             dcl        var(&G02TGI)   type(*dec)  len(5 0) +
005400050301                          value(0)
005500050301             dcl        var(&G02PAR)   type(*char) len(22)
005600050405    /*?Parametri per pgm. XSRGI8?*/
